{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["../../src/*"],
      "@raubjo/architect-core": ["../../src/index.ts"],
      "@raubjo/architect-core/*": ["../../src/*"]
    },
    "types": ["vite/client"],
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["."]
}
